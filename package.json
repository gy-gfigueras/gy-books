{
  "name": "WingWords",
  "version": "0.1.15",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "node scripts/dev-with-banner.js",
    "dev-simple": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky install",
    "precommit": "lint-staged",
    "test": "jest --coverage",
    "test:coverage": "jest --coverage --reporters=default --reporters=jest-junit",
    "test:watch": "jest --watch",
    "test:ci": "jest --coverage --watchAll=false",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:run:production": "cypress run --config baseUrl=https://wingwords.gycoding.com",
    "test:e2e": "cypress run",
    "test:e2e:production": "cypress run --config baseUrl=https://wingwords.gycoding.com"
  },
  "dependencies": {
    "@auth0/nextjs-auth0": "^4.10.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@gycoding/nebula": "^1.0.10",
    "@mui/icons-material": "^7.1.1",
    "@mui/material": "^7.3.4",
    "@mui/x-charts": "^8.8.0",
    "@reduxjs/toolkit": "^2.8.2",
    "@types/testing-library__jest-dom": "^5.14.9",
    "eslint-config-prettier": "^10.1.1",
    "framer-motion": "^12.23.12",
    "husky": "^9.1.7",
    "lottie-react": "^2.4.1",
    "next": "^15.5.3",
    "prettier": "^3.5.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^9.2.0",
    "redoc": "^2.5.0",
    "swagger-jsdoc": "^6.2.8",
    "swr": "^2.3.3"
  },
  "devDependencies": {
    "@commitlint/cli": "19.8.0",
    "@commitlint/config-conventional": "19.8.0",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.22.0",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/testing-library__react": "^10.0.1",
    "@types/uuid": "^10.0.0",
    "eslint": "^8.57.1",
    "eslint-config-next": "^15.5.3",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-react": "^7.37.4",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.0.5",
    "jest-junit": "^16.0.0",
    "lint-staged": "15.5.0",
    "mongodb": "^6.17.0",
    "prettier": "^3.5.3",
    "start-server-and-test": "^2.0.12",
    "ts-jest": "^29.4.1",
    "typescript": "^5",
    "typescript-eslint": "^8.26.1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ],
    "next-env.d.ts": []
  },
  "jest-junit": {
    "output": "coverage/test-report.xml"
  }
}
